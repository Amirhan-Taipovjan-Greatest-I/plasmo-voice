{
    "schemaVersion": 1,
    "id": "plasmovoice",
    "version": "${version}",
    "name": "Plasmo Voice",
    "description": "A proximity voice chat mod with audio positioning and lots of features",
    "authors": [
        "Plasmo"
    ],
    "contact": {
        "homepage": "https://github.com/plasmoapp/plasmo-voice",
        "sources": "https://github.com/plasmoapp/plasmo-voice",
        "issues": "https://github.com/plasmoapp/plasmo-voice/issues"
    },
    "license": "LGPL-3.0",
    "icon": "assets/plasmovoice/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            "su.plo.voice.fabric.client.FabricVoiceClient"
        ],
        "main": [
            "su.plo.voice.fabric.server.FabricVoiceServer"
        ]
    },
    "mixins": [
        "plasmovoice-common.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}"
    },
    "suggests": {
        "soundphysics": "*"
    }
}
